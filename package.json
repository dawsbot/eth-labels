{
  "name": "evm-labels",
  "version": "0.0.3",
  "description": "A public dataset of Etherscan labels",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "vitest": "vitest",
    "test": "npm run lint && npm run vitest -- --run && npm run build",
    "lint": "eslint . --fix --cache",
    "build": "rm -rf lib && npm run compile",
    "pull": "tsx scripts/fetch-all.ts",
    "build:json": "node scripts/build-json.js",
    "prepare": "husky install",
    "compile": "npm-run-all --parallel tsc:esm tsc:cjs",
    "tsc:esm": "tsc -p tsconfig.json",
    "tsc:cjs": "tsc -p tsconfig-cjs.json",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run build",
    "markdown-magic": "npx tsx docs/markdown-magic.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dawsbot/evm-labels.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/dawsbot/evm-labels/issues"
  },
  "homepage": "https://github.com/dawsbot/evm-labels#readme",
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ]
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.5",
    "@types/markdown-magic": "^1.0.4",
    "@types/node": "^20.11.30",
    "cheerio": "^1.0.0-rc.12",
    "cli-progress": "^3.12.0",
    "csv-parse": "^5.0.4",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "markdown-magic": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.42.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "rollup": "^4.13.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.3.1",
    "vitest": "^1.4.0",
    "zod": "^3.22.4"
  },
  "lint-staged": {
    "./data/**/*.json": "npx tsx scripts/combine.ts",
    "*": "prettier -u --write"
  }
}
